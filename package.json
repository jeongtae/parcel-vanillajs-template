{
  "name": "parcel-vanillajs-template",
  "version": "1.0.0",
  "license": "MIT",
  "source": "src/index.html",
  "browserslist": "> 1%, ie 11, not dead",
  "targets": {
    "default": {
      "distDir": "dist",
      "publicUrl": "/",
      "scopeHoist": true,
      "optimize": true,
      "sourceMap": true
    }
  },
  "scripts": {
    "start": "parcel",
    "build": "parcel build",
    "build:no-minify": "parcel build --no-optimize --no-content-hash",
    "clean": "rimraf dist .parcel-cache"
  },
  "devDependencies": {
    "parcel": "^2.0.0",
    "postcss": "^8.3.11",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2"
  }
}
